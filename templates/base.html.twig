<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        <link href="{{ asset('assets/main/css/style.css') }}" type="text/css" rel="stylesheet"/>

        <!-- Bootstrap4 files-->
        <link href="{{ asset('assets/main/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet"/>


        <!-- custom style -->
        <link href="{{ asset('assets/main/css/ui.css') }}" type="text/css" rel="stylesheet"/>
        <link href="{{ asset('assets/main/css/responsive.css') }}" rel="stylesheet"
              media="only screen and (max-width: 1200px)"/>

        <!-- our styles -->

        <link href="{{ asset('assets/main/css/styles.css') }}" rel="stylesheet" type="text/css"/>


    {% endblock %}

    {% block javascripts %}
        {#  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script> #}
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
        <script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
    {% endblock %}
</head>
<body>
{% include 'parts/header.html.twig' %}

{# flash for verification successful #}
{% for message in app.flashes('verified') %}
    <div class="alert alert-success">
        {{ message }}
    </div>
{% endfor %}

{# flash for the sending of the verification link #}
{% for message in app.flashes('sent') %}
    <div class="alert alert-warning">
        {{ message }}
    </div>
{% endfor %}



<div class="container-fluid mb-5">
    {% block body %}

    {% endblock %}
</div>
{% include 'parts/footer.html.twig' %}
</body>
</html>